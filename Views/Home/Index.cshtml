@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}

<div class="col-lg-12" ng-app="myApp" ng-controller="tagging">
    <div class="col-lg-3 col-md-3" style="border: 1px solid #0094ff; border-radius: 5px; padding: 5px; float: left">
        <label class="">Control Number</label><br />
        <input id="controlnum" class="form-control-md" type="text" />
        <button id="getimg" class="btn btn-dark col-md-4">Get</button>
        <hr />
        <label class="col-md-4">Main Tag</label>
        <select id="maintag" class="form-control-md col-md-7">
            <option ng-repeat="r in rows" value="{{r.maintagid}}">{{r.maintagval}}</option>
        </select>
        <br />
        <label class="col-md-4">Sub Tag</label>
        <select id="subtag" class="form-control-md col-md-7">
            <option></option>
        </select>
        <br />
        <hr />
        <label class="col-md-12">Index Fields</label>
        <hr />
        <div id="divresult"></div>

    </div>

    <div class="col-lg-9 col-md-9" style="border:1px solid #0094ff;border-radius:5px;padding:5px;float:left">
        <iframe id="viewer" src=""></iframe>
    </div>
</div>
